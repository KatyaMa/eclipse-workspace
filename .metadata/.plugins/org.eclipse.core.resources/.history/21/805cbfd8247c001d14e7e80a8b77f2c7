package Jump;

public class Jump {

	public static void main(String[] args) {
		// int flagHeight = 8;
		// int bigJump = 3;
		jump(8, 3);
	}
		public static int jump(int flagHeight, int bigJump) {
        int count = 0;
        count += (int)(flagHeight / bigJump + (flagHeight % bigJump));
        return count; 

	}

}
